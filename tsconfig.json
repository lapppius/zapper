{
	"compilerOptions": {
		"forceConsistentCasingInFileNames": true,
		"target": "ES2022",
		"paths": {
			"@/*": ["./src/*"]
		},
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": false,
		"noEmit": true,
		"incremental": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"plugins": [
			{
				"name": "next"
			}
		]
	},
	"include": [
		"next-env.d.ts",
		".next/types/**/*.ts",
		"**/*.ts",
		"**/*.tsx",
		"src/app/(auth)/login/page.jsx"
, "src/app/(public)/page.jsx", "src/app/lib/postgres/getRadios.js", "src/auth.config.ts", "src/app/test/page.tsx", "src/app/api/plays/route.js"	],
	"exclude": ["node_modules"]
}
